net : "train12.prototxt"
# net : "train12_no_pts.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.01  #0.0001
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 30000
